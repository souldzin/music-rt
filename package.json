{
  "name": "server-frontend",
  "version": "1.0.0",
  "description": "This is the front end server for music-rt",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "js": "browserify ./src/app.js -o ./www/app.bundle.js -t [ babelify --presets [ es2015 react ] ] -p [ rememberify ]",
    "js:watch": "onchange \"./src/**/!(*.test).js\" -- npm run js",
    "test": "mocha \"./src/**/*.test.js\"",
    "test:watch": "onchange \"./src/**/*.js\" -- npm run test",
    "dev:server": "lite-server -c dev-server.json",
    "dev": "concurrently \"npm run test:watch\" \"npm run js:watch\" \"npm run dev:server\""
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "chai": "^4.0.2",
    "concurrently": "^3.4.0",
    "lite-server": "^2.3.0",
    "mocha": "^3.4.2",
    "onchange": "^3.2.1",
    "rememberify": "^1.0.1",
    "sinon": "^2.3.4"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "tone": "^0.10.0"
  }
}
