{
  "name": "music-rt",
  "version": "1.0.0",
  "description": "",
  "main": "src/ui/main.js",
  "license": "MIT",
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "scripts": {
    "build:js": "browserify ./src/ui/main.js -o ./dist/main.bundle.js",
    "watch:js": "watchify ./src/ui/main.js -o ./dist/main.bundle.js -v",
    "build:css": "lessc --clean-css ./src/styles/main.less ./dist/main.bundle.css",
    "watch:css": "onchange -v ./src/**/*.less -- npm run build:css",
    "build": "npm run build:js && npm run build:css",
    "watch": "concurrently --kill-others \"npm run watch:js\" \"npm run watch:css\"",
    "clean": "trash ./dist/*.bundle.*"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "chai": "^4.0.2",
    "concurrently": "^3.4.0",
    "less": "^2.7.2",
    "less-plugin-clean-css": "^1.5.1",
    "mocha": "^3.4.2",
    "onchange": "^3.2.1",
    "sinon": "^2.3.4",
    "trash-cli": "^1.4.0",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.4.2",
    "tone": "^0.10.0",
    "uuid": "^3.1.0"
  }
}
